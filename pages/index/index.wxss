}
.menuList{
  margin: 34rpx 30rpx 0 30rpx;
  background: #fff;
  min-height: 183rpx;
  overflow: hidden;
  text-align: center;
  padding-bottom: 30rpx;
}
.menuList .menuItem{
  width: 25%;
  height: 96rpx;
  text-align: center;
  float: left;
  margin-top: 26rpx;
  position: relative;
}
.menuList .menuItem .img{
  width: 100%;
  height: 64rpx;
  text-align: center;
}
.menuList .menuItem .img image{
  height: 64rpx;
}
.menuItem .text{
  margin-top: 8rpx;
  font-size: 24rpx;
  color: #252525;
}
.menuItem .spot{
  width: 16rpx;
  height: 16rpx;
  background: #D90C18;
  color: #fff;
  line-height: 28rpx;
  text-align: center;
  position: absolute;
  right: 30rpx;
  top: 0rpx;
  border-radius: 8rpx;
  font-size: 20rpx;
}
.menuGrid .icon{
  position: relative;
}
.menuGrid .spot{
  width: 28rpx;
  height: 28rpx;
  background: #D90C18;
  color: #fff;
  line-height: 28rpx;
  text-align: center;
  position: absolute;
  right: -15rpx;
  top: 0rpx;
  border-radius: 14rpx;
  font-size: 20rpx;
}
.spotCur{
  line-height: 16rpx !important;
  font-weight: bold;
}
.carInfo{
  /* position: relative; */
  margin: 30rpx 30rpx 0 30rpx;
  height: 290rpx;
  box-shadow: 0px 0px 10px rgba(196, 196, 196, 0.45);
  border-radius: 12rpx;
  overflow: hidden;
}
.car{
  height: 280rpx;
  width: 100%;
  /* position: absolute;
  left: 0;
  top: -220rpx;
  border-radius: 10rpx; */
}
.car .info{
  height: 156rpx;
  background: #fff;
  /* border-radius: 12rpx; */
}
.car .info .left{
  float: left;
  width: 96rpx;
  height: 96rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  /* background: #F8F8F8; */
  border-radius: 12rpx;
  text-align: center;
}
.car .info .left image{
  width: 76rpx;
  height: 76rpx;
  margin-top: 10rpx;
}
.car .info .center{
  float: left;
  margin-top: 42rpx;
  margin-left: 16rpx;
}
.car .info .center .carNum{
  position: relative;
  font-size: 32rpx;
  font-weight: bold;
  overflow: hidden;
}
.car .info .center .carNum view{
  float: left;
}
.car .info .center .carNum .label{
  margin-left: 12rpx;
  font-size: 20rpx;
  font-weight: 500;
  padding: 5rpx 20rpx;
  color: #FFFFFF;
  background: linear-gradient(129deg, #FFA271 0%, #F9752F 100%);
  border-radius: 16rpx 16rpx 16rpx 0px;
}
.car .info .center .model{
  width: 400rpx;
  font-size: 20rpx;
  color: #666;
  margin-top: 10rpx;
}
.car .info .right {
  float: right;
  width: 200rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 99;
  text-align: right;
  /* margin-top: 40rpx;
  margin-right: 26rpx; */
}
.car .info .right image{
  width: 40rpx;
  height: 40rpx;
  margin-top: 60rpx;
  margin-right: 36rpx;
}
.car .info .left image{
  width: 80rpx;
  height: 80rpx;
}
.car .integral{
  height: 252rpx;
  width: 100%;
  background: #fff;
