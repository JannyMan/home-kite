  font-size: 24rpx;
  color: #FFFFFF;
}
.integralBtn .reportbtn1{
  width: 308rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: 1px solid #D90C18;
  border-radius: 12rpx;
  font-size: 24rpx;
  color: #D90C18;
