}
.integralBtn .newbtn{
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: 2rpx solid #D90C18;
  border-radius: 12rpx;
  font-size: 24rpx;
  color: #D90C18;
  box-sizing: border-box;
}

.menuIcon{
