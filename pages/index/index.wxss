  height: 76rpx;
  border-right: 1px solid #eee;
}
.car .integralInfo .van-row .van-col:last-child{
  border: none;
}
.car .integralInfo .van-row .van-col .integralTitle{
  color: #252525;
  font-size: 24rpx;
}
.integralNum{
    margin-top: 14rpx;
    padding: 0 12rpx;
    display: flex;
    justify-content: center;
}
.integralNum .num{
  display: inline-block;
  font-size: 26rpx;
  line-height: 28rpx;
  font-weight: bold;
  position: relative;
}
